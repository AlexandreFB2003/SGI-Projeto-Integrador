generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model Professor {
  nome String 
  codigo String @id
  senha String
  cpf String
  telefone String?
  email String?
  dt_nascimento String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@map("professores")
}