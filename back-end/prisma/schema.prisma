generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model Professor {
  nome String @id
  codigo String
  senha String
  cpf BigInt
  telefone String?
  email String?
  dt_nascimento String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@map("professores")
}